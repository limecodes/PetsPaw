{
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "import"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "react-app",
    "airbnb-typescript",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/jsx-props-no-spreading": "warn",
    "react/prop-types": "off",
    "no-console": "error",
    "no-nested-ternary": "warn"
  }
}
